# .gitattributes
# always use Unix LF
.gitattributes eol=lf
.gitignore eol=lf
Makefile text eol=lf
*.yml text eol=lf
*.c text eol=crlf
*.h text eol=crlf
# GIT LFS
*.jpg filter=lfs diff=lfs merge=lfs -text
*.ods filter=lfs diff=lfs merge=lfs -text
*.dwf3work filter=lfs diff=lfs merge=lfs -text
*.gif filter=lfs diff=lfs merge=lfs -text
